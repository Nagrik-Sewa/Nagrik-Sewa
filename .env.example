# Gemini AI Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Add your Google AI API key here
# Get it from: https://makersuite.google.com/app/apikey
# Example: VITE_GEMINI_API_KEY=AIzaSyBxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Google OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your_google_oauth_client_id_here

# Add your Google OAuth Client ID here
# Get it from: https://console.developers.google.com/
# Example: VITE_GOOGLE_CLIENT_ID=123456789012-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx.apps.googleusercontent.com

# Database Configuration
DATABASE_URL=mongodb://localhost:27017/nagrik-sewa

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRES_IN=7d
REFRESH_TOKEN_SECRET=your-refresh-token-secret-here
REFRESH_TOKEN_EXPIRES_IN=30d

# Email Configuration (for notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password

# SMS Configuration (for OTP)
SMS_API_KEY=your-sms-api-key
SMS_SENDER_ID=NAGRIK

# App Configuration
NODE_ENV=development
PORT=8080
CLIENT_URL=http://localhost:8080
PING_MESSAGE=Nagrik Sewa API is running

# Security
BCRYPT_ROUNDS=12
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=900000

# File Upload
MAX_FILE_SIZE=5242880
UPLOAD_PATH=./uploads

# Redis (optional - for session storage)
REDIS_URL=redis://localhost:6379
